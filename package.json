{
  "name": "electron-hybrid-boilerplate",
  "appName": "com.electron-hybrid.boilerplate",
  "productName": "Electron Hybrid Boilerplate",
  "description": "Electron Hybrid Boilerplate Description",
  "version": "0.0.1",
  "protocolClient": "eltrhbd",
  "main": "./.webpack/main",
  "scripts": {
    "dev": "env-cmd -e default,dev electron-forge start",
    "stagex": "env-cmd -e default,stagex electron-forge start",
    "prod": "env-cmd -e default,prod electron-forge start",
    "start": "env-cmd -e default,dev electron-forge start",
    "build:app": "env-cmd -e default,prod webpack --config .inc/webpack/webpack.config.js",
    "build:exe": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-squirrel --arch=ia32",
    "build:exe-x64": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-squirrel --arch=x64",
    "build:msi": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-wix --arch=ia32",
    "build:msi-x64": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-wix --arch=x64",
    "build:dmg": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-dmg --arch=universal",
    "build:dmg-arm64": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-dmg --arch=arm64",
    "build:dmg-x86": "env-cmd -e default,prod electron-forge make --targets @electron-forge/maker-dmg --arch=x86",
    "package:app": "serve -p 4000 ./build/",
    "package:electron": "env-cmd -e default,prod electron-forge package",
    "publish:electron": "env-cmd -e default,prod electron-forge publish",
    "lint": "eslint 'src/**/*.js' || true",
    "clean": "rimraf .webpack .build .dist out build",
    "reinstall": "rimraf ./node_modules && yarn clean && yarn install"
  },
  "keywords": [],
  "author": "electron-hybrid-boilerplate",
  "license": "MIT",
  "config": {
    "forge": "./.inc/config/forge.config.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "@datadog/datadog-api-client": "^1.5.0",
    "@fingerprintjs/fingerprintjs": "^3.3.6",
    "antd": "^4.24.1",
    "blob-util": "^2.0.2",
    "core-js": "^3.26.0",
    "cross-fetch": "^3.1.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-util": "^0.17.2",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-transform-arrow-functions": "^7.18.6",
    "@babel/plugin-transform-async-to-generator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-dmg": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-wix": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.67",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "electron": "^21.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-wix-msi": "^4.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-permissions-plugin": "^1.0.8"
  }
}
